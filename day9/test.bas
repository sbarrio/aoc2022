5 SCREEN 0: KEYOFF: WIDTH 40
10 CLS: DEFINT A-Z: HX = 0: HY = 0: TX = 0: TY = 0
20 READ R$
30 IF R$ = "*" GOTO 100
40 READ A
50 IF R$ = "U" THEN HY = HY - A
60 IF R$ = "R" THEN HX = HX + A
70 IF R$ = "L" THEN HX = HX - A
80 IF R$ = "D" THEN HY = HY + A
90 GOTO 20
100 PRINT "Head:"HX","HY" Tail:"TX","TY
110 STOP

500 DATA"R", 4
501 DATA "U", 4
502 DATA "L", 3
503 DATA "D", 1
504 DATA "R", 4
505 DATA "D", 1
506 DATA "L", 5
507 DATA "R", 2
508 DATA "*"